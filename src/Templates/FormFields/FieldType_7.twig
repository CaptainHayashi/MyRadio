<input type="hidden" name="{{frm_name}}-{{sfieldname}}" id="{{frm_name}}-{{field.name}}" value="{{field.value}}" />
<label for="{{frm_name}}-{{field.name}}-ui">{{field.label}}</label>
<input type="text" class="{{field.class}}{% if field.required %} required{% endif %} text artist-autocomplete"
       name="{{frm_name}}-{{field.name}}-ui" id="{{frm_name}}-{{field.name}}-ui" value="{{field.options.artistname}}" />
<span class="myuryfrmhint" id="{{frm_name}}-{{field.name}}-hint">{{field.explanation | raw}}</span>