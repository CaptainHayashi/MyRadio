<label for="{{frm_name}}-{{field.name}}">{{field.label}}</label>
<input type="text" class="{{field.class}}{% if field.required %} required{% endif %} datetime"
       value="{{field.value}}" name="{{frm_name}}-{{sfieldname}}" id="{{frm_name}}-{{field.name}}"
       {% if not field.enabled %} readonly="readonly"{% endif %} placeholder="Day/Month/Year Hour:Mins" />
<span class="myuryfrmhint" id="{{frm_name}}-{{field.name}}-hint">{{field.explanation | raw}}</span>