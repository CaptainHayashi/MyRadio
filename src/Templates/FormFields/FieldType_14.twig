<label for="{{frm_name}}-{{field.name}}">{{field.label}}</label>
<input type="text" class="{{field.class}}{% if field.required %} required{% endif %} time"
       value="{{field.value}}" name="{{frm_name}}-{{field.name}}" id="{{frm_name}}-{{field.name}}"
       {% if not field.enabled %} readonly="readonly"{% endif %} />
<span class="myuryfrmhint" id="{{frm_name}}-{{field.name}}-hint">{{field.explanation | raw}}</span>