{% extends 'stripe.twig' %}
{% block stripecontent %}
<h2>{{profile_name}}</h2>
<div class="timeline">
{% for event in timeline %}
<div class="timeline-event">
  <div class="timeline-spine">&nbsp;</div>
  {{event.timestamp}}
    <heading>{{profile_name}} {{event.message}}</heading>
  </div>
{% endfor %}
</div>
{% endblock %}