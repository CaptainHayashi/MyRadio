{% extends 'stripe.twig' %}
{% block stripecontent %}
<h2>{{profile_name}}</h2>
<div class="timeline">
  <div class="timeline-spine">&nbsp;</div>
{% for event in timeline %}
  <div class="timeline-event">
  {{event.timestamp}}
      <heading>{{profile_name}} {{event.message}}</heading>
    </div>
{% endfor %}
  <div class="clear">&nbsp;</div>
  </div>
{% endblock %}