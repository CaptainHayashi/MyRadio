{% extends 'base.twig' %}
{% block content %}
<div id="content-stripe" class="clearfix">
  {% for notice in notices %}
    <div class="ui-state-{{notice.state}}"><span class="ui-icon ui-icon-{{notice.icon}} fleft"></span>{{notice.message}}</div>
  {% endfor %}
    {% if submenu|length > 0 %}
    {% include 'parts/submenu.twig' %}
    {% endif %}
  {% block stripecontent %}{{stripedata | raw}}{% endblock %}
    </div>
{% endblock %}