{% extends 'base.twig' %}
{% block content %}
<header id="pageHeader" class="clearfix">
  <a id="logo" href="{{baseurl}}">
    <h1>{{config.long_name}}</h1>
  </a>
</header>
<div id="grid" class="transBG clearfix">
  <header id="content-header">
    <h2>{{ title }}</h2>
  </header>
  <div id="content-body" class="clearfix">
  {% for notice in notices %}
      <div class="ui-state-{{notice.state}}"><span class="ui-icon ui-icon-{{notice.icon}} fleft"></span>{{notice.message | raw}}</div>
  {% endfor %}
    {% if submenu|length > 0 %}
    {% include 'parts/submenu.twig' %}
    {% endif %}
  {% block stripecontent %}{{stripedata | raw}}{% endblock %}
      </div>
{% endblock %}
