{% extends 'stripe.twig' %}
{% block head %}
{{ parent() }}
<script type="text/javascript" src="//ury.york.ac.uk/portal/js/jquery-ui-timepicker.js"></script>
<script type="text/javascript" src="//ury.york.ac.uk/portal/js/jquery.validate.min.js"></script>
<script type="text/javascript" src="js/myury.form.js"></script>
{% endblock %}
{% block stripecontent %}
<fieldset class="{{frm_classes}}">
  <form action="{{frm_action | raw}}" method="{{frm_method}}" name="{{frm_name}}" id="{{frm_name}}">
    {% for field in frm_fields %}
      <div id="{{frm_name}}-{{field.name}}-container" class="{{field.class}}">
        {% include ('FormFields/FieldType_'~field.type~'.twig') %}
        </div>
    {% endfor %}
        <input type="submit" value="Submit" />
      </form>
    </fieldset>
{% endblock %}